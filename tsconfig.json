{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2017",
    "lib": [
      "esnext"  //error TS2304: Cannot find name 'AsyncIterator'.
      ,"dom"    //error TS2304: Cannot find name 'Blob'.
    ],    
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
